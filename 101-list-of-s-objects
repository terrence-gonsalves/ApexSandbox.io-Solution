public List<sObject> getListOfsObject(List<Account> accounts, List<Contact> contacts)
{
    List<sObject> objectsList = new List<sObject>();

    // if (accounts?.size() == 0 && contacts?.size() == 0) {
    //     return objectsList;
    // }

    objectsList.addAll(accounts);
    objectsList.addAll(contacts);

    return objectsList;
}