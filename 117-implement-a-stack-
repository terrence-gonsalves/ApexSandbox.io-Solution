public class Stack {
    List<Object> stack = new List<Object>();
    
    
    public void push(Object obj) {
        stack.add(obj);
    }
    
    public Object pop() {
       return stack.remove(stack.size() - 1);
    
    public Integer size() {
       return stack.size();
    }
    
    public Object peek(){
        return stack.get(stack.size() - 1);
    }
    
    public Boolean isEmpty() {
       if (stack.isEmpty()) {
           return true;
       }

       return false;
    }
}