public void defragging(Integer[] memory)
{
    if (memory.size() == 0) return;

    Integer temp, startPointer = 0, counter = 0;


    while (counter < memory.size() - 1) { 
        temp = memory.get(startPointer);

        if (temp == -1) {  
            memory.remove(startPointer);
            memory.add(temp);
        }
        
        counter++;
    }
}