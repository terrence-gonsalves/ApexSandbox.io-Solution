public Integer secondLargest(List<Integer> numbers) {
    Set<Integer> numbersSet = new Set<Integer>();
    List<Integer> orderedNumbers = new List<Integer>();

    numbersSet.addAll(numbers);
    orderedNumbers.addAll(numbersSet);
    orderedNumbers.sort();
    
    return orderedNumbers.get(orderedNumbers.size() - 2);
}