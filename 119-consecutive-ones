public Integer maxConsecutiveOnes(Integer[] numbers) {
    Integer onesTotal = 0;

    if (numbers.size() == 0) return onesTotal;

    for (Integer x = 0; x < numbers.size(); x++) {
        if (numbers[x] == 1) {
            onesTotal++;
        } else {
            onesTotal = 0;
        }
    }

    return onesTotal;
}